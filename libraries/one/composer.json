{
    "name": "drjele-monorepo/library-one",
    "type": "library",
    "require": {
        "php": "^8.1",
        "drjele/doctrine-type": "^3.0"
    },
    "require-dev": {
        "drjele/symfony-phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Monorepo\\Library\\One\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monorepo\\Library\\One\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
